//carrega um save file...eu sei...mind blown .-.

ini_open("save.ini");
if (ini_read_real("Exists","exist",0) == 0) {
    show_message("No save file found, creating a new one .-.");
    ini_write_real("Exists","exist",1);
    
    for(i = 1;i &lt;= 5;i++) {
        ini_write_real("Character" + string(i),"xp",0);
        ini_write_real("Character" + string(i),"type",-1);
    }
    
    ini_close();
    exit;
}


//init das characters do jogo(as que n foram criadas vao tar blank--&gt; type = -1)

for (i = 1;i &lt;= 5;i++) {
    with(character[i]) {
        xp = ini_read_real("Character" + string(i),"xp",0);
        type = ini_read_real("Character" + string(i),"type",-1);
    }
}
ini_close();


show_debug_message(character[1].type);

for (i = 1;i &lt;= 5;i++){
    if(i==1){character[i].selected = true;}
    character[i].x = (view_wport[0]/6)*i;
    character[i].y = 130;
    switch(character[i].type){
        case -1: character[i].sprite_index = spr_empty; break;
        case 1: character[i].sprite_index = spr_fucker1; break;
    }
    character[i].image_speed = 0;
}








